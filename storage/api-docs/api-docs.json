{
    "openapi": "3.0.0",
    "info": {
        "title": "Bug Tracker API",
        "description": "Dokumentasi REST API Bug Tracker untuk QA, DEV, dan PM.",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Local development server"
        }
    ],
    "paths": {
        "/api/bugs/{bugId}/attachments": {
            "get": {
                "tags": [
                    "Attachments"
                ],
                "summary": "List semua attachment di bug tertentu",
                "description": "Menampilkan daftar semua file yang dilampirkan pada bug, termasuk URL publik untuk diakses client app.",
                "operationId": "2089891a07344d6f53e5f75bcd16a655",
                "parameters": [
                    {
                        "name": "bugId",
                        "in": "path",
                        "description": "ID bug",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List attachments berhasil diambil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "filename": {
                                                        "type": "string",
                                                        "example": "screenshot.png"
                                                    },
                                                    "mime_type": {
                                                        "type": "string",
                                                        "example": "image/png"
                                                    },
                                                    "file_size": {
                                                        "type": "integer",
                                                        "example": 204856
                                                    },
                                                    "url": {
                                                        "type": "string",
                                                        "example": "http://localhost:8000/storage/attachments/bug_1/screenshot.png"
                                                    },
                                                    "uploaded_by": {
                                                        "properties": {
                                                            "id": {
                                                                "type": "integer",
                                                                "example": 2
                                                            },
                                                            "name": {
                                                                "type": "string",
                                                                "example": "QA User"
                                                            }
                                                        },
                                                        "type": "object"
                                                    },
                                                    "uploaded_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-12-01T10:30:00Z"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Bug tidak ditemukan"
                    },
                    "401": {
                        "description": "Tidak terautentikasi"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Attachments"
                ],
                "summary": "Upload file attachment ke bug",
                "description": "Upload file (screenshot, log, dll) ke bug tertentu. File akan disimpan di storage dan URL publik akan dikembalikan.",
                "operationId": "77d1b1a2b89ed2db0d54ebf8c7df9a6a",
                "parameters": [
                    {
                        "name": "bugId",
                        "in": "path",
                        "description": "ID bug",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "requestBody": {
                    "description": "File yang akan diupload",
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "file"
                                ],
                                "properties": {
                                    "file": {
                                        "description": "File attachment (max 10MB, format: jpg, jpeg, png, gif, pdf, txt, log, doc, docx)",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "File berhasil diupload",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "File uploaded successfully"
                                        },
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "filename": {
                                                    "type": "string",
                                                    "example": "screenshot.png"
                                                },
                                                "mime_type": {
                                                    "type": "string",
                                                    "example": "image/png"
                                                },
                                                "file_size": {
                                                    "type": "integer",
                                                    "example": 204856
                                                },
                                                "url": {
                                                    "type": "string",
                                                    "example": "http://localhost:8000/storage/attachments/bug_1/1733052840_screenshot.png"
                                                },
                                                "uploaded_by": {
                                                    "properties": {
                                                        "id": {
                                                            "type": "integer",
                                                            "example": 2
                                                        },
                                                        "name": {
                                                            "type": "string",
                                                            "example": "QA User"
                                                        }
                                                    },
                                                    "type": "object"
                                                },
                                                "uploaded_at": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validasi gagal (file terlalu besar atau format tidak didukung)"
                    },
                    "404": {
                        "description": "Bug tidak ditemukan"
                    },
                    "401": {
                        "description": "Tidak terautentikasi"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
